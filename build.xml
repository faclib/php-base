<?xml version="1.0" encoding="utf-8" ?>
<project name="php-base-utils" default="phpab" basedir=".">
    <target name="phpab">
        <exec executable="phpab">
            <arg value="--output" />
            <arg path="src/__autoload.php" />
            <arg value="--basedir" />
            <arg path="src" />
            <arg path="src/lib" />
        </exec>
    </target>
</project>